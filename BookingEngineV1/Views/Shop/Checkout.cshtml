@model Cart

    @if (Model != null)
    {
        if (Model.CartItems != null && Model.CartItems.Count > 0)
        {
            @Html.Partial("_CartSummary", Model)

            <form asp-action="Checkout" asp-controller="Boockings" method="post">
                <input type="text"  name="cartID" value="@Model.CartID"/>
                <button  type="submit">Checkout</button>
            </form>
        }

    }
